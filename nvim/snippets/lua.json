{
  "Ignore stylua": {
    "prefix": [
      "ignore"
    ],
    "body": [
      "-- stylua: ignore"
    ]
  },
  "Pretty print": {
    "prefix": [
      "P"
    ],
    "body": [
      "vim.print($0)"
    ]
  },
  "Print table": {
    "prefix": [
      "pp"
    ],
    "body": [
      "print(vim.inspect($0))"
    ]
  }
}
